function difference(arr1, arr2) {
  var commonItems = [];
  for (let i = 0; i < arr1.length; i++) {
    for (let j = 0; j < arr2.length; j++) {
      if (arr1[i] == arr2[j]) {
        commonItems.push(arr1[i]);
      }
    }
  }

  return commonItems;
}
console.log(difference([1, 2, 3], [100, 2, 1, 10]));
